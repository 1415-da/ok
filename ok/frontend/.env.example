# API Configuration
VITE_API_URL=http://localhost:8080/api

# Application Configuration
VITE_APP_NAME=YellowSense Confidential Clean Room
VITE_APP_VERSION=1.0.0

# Environment
VITE_NODE_ENV=development

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_DEBUG=true

# Executor Service (for direct communication if needed)
VITE_EXECUTOR_URL=http://localhost:8443

# Upload Configuration
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.csv,.json,.txt

# UI Configuration
VITE_DEFAULT_USER_ID=Auditor
VITE_THEME=light

# Timeouts (in milliseconds)
VITE_API_TIMEOUT=600000
VITE_UPLOAD_TIMEOUT=300000

# Polling Intervals (in milliseconds)
VITE_LOG_POLL_INTERVAL=2000
VITE_STATUS_POLL_INTERVAL=5000
